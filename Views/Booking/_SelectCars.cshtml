@using static AmiFlota.Utilities.Enums
@model List<AmiFlota.Models.CarModel>

@{
    TrunkType tempValue = new TrunkType();

    @foreach (var car in Model)
    {
        if (!car.Trunk.Equals(tempValue))
        {

            <h5 class="cente">@car.Trunk</h5>
            tempValue = @car.Trunk;
        }

        <div class="form-check">
            <input class="form-check-input" type="checkbox" value="@car.VIN" id="@car.VIN" checked="checked" />
            <label class="form-check-label">@car.Brand @car.Model @car.RegistrationNumber</label>
        </div>
    }
}


<script src="~/js/script-custom-calendar.js"></script>