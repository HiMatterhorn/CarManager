@model AmiFlota.Models.ViewModels.CalendarVM

<div class="modal fade modal-xl" role="dialog" id="calendarEvent" data-backdrop="false" data-keyboard="false">
    <div class="modal-dialog">
        <div class="modal-content ">


            <div class="modal-header">
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body m-2">

                <div class="row">
                    <partial name="_BookingListItem" model=@Model.Booking class="mx-0 px-0" />
                </div>

                <div class="row">
                    <partial name="_TripsHistory" model=@Model.TripsHistory class="mx-0 px-0" />
                </div>
            </div>

        </div>
    </div>
</div>