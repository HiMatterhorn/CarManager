@model List<AmiFlota.Models.CarModel>
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Calendar</title>
</head>
<body>

@*    <div class="container">*@
        <div class="row">
            <div class="col-xl-4 col-lg-4 col-md-2 col-sm-6 col-12 order-1">
                @foreach (var car in Model)
                {

                    <div class="form-check">
                        <input class="form-check-input" type="checkbox" value="@car.VIN" id="@car.VIN" checked="checked" />
                        <label class="form-check-label">@car.Brand @car.Model @car.RegistrationNumber</label>
                    </div>
                }

            </div>
            <div class="col-xl-8 col-lg-8 col-md-10 col-sm-6 col-12 order-2">
                <div id="calendar"> </div>
            </div>
        </div>
@*    </div>*@

    <partial name="_CalendarEvent" />

</body>

</html>

